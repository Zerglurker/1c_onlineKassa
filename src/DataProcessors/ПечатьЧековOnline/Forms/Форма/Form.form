  <attributes>
    <name>Список</name>
    <title>
      <key>ru</key>
      <value>Список</value>
    </title>
    <id>2</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	d.Ссылка КАК Ссылка,
	d.ПометкаУдаления КАК ПометкаУдаления,
	d.Номер КАК Номер,
	d.Дата КАК Дата,
	d.Проведен КАК Проведен,
	d.Контрагент КАК Контрагент,
	d.ТипОперации КАК ТипОперации,
	d.Основание КАК Основание,
	d.ФискальныйНомер КАК ФискальныйНомер,
	d.ФискальныйАттрибут КАК ФискальныйАттрибут,
	d.Обработан КАК Обработан,
	d.КонтрольнаяСумма КАК КонтрольнаяСумма,
	tSum.Сумма КАК Сумма,
	ВЫБОР
		КОГДА НЕ d.Обработан
				И iExec.Чек ЕСТЬ NULL
			ТОГДА &quot;подготовлен&quot;
		КОГДА НЕ d.Обработан
				И НЕ iExec.Чек ЕСТЬ NULL
			ТОГДА &quot;обрабатываеться&quot;
		КОГДА d.Обработан
			ТОГДА &quot;обработан&quot;
	КОНЕЦ КАК Статус,
	iExec.id КАК idЗадания,
	iExec.Дата КАК ДатаПечати
ИЗ
	Документ.КассовыйЧек КАК d
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ЧекиВОбработке КАК iExec
		ПО d.Ссылка = iExec.Чек
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			t.Ссылка КАК Чек,
			СУММА(t.Сумма) КАК Сумма
		ИЗ
			Документ.КассовыйЧек.Товары КАК t
		
		СГРУППИРОВАТЬ ПО
			t.Ссылка) КАК tSum
		ПО d.Ссылка = tSum.Чек
ГДЕ
	(НЕ d.Обработан
			ИЛИ НЕ iExec.Чек ЕСТЬ NULL)
	И НЕ d.ПометкаУдаления</queryText>
      <mainTable>Document.КассовыйЧек</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
